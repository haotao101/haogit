
.imgDad{
	width: 58px;
	height: 58px;
	position: fixed;
	bottom: 23px;
	bottom: calc(23px + env(safe-area-inset-bottom));/*兼容ios*/
	left:50%;
	-webkit-transform: translateX(-50%);
	    -ms-transform: translateX(-50%);
	        transform: translateX(-50%);
	border-radius: 50%;
	background: transparent;
	z-index: 3000;
}
.imgDad:before{
	content: '';
	position: absolute;
	bottom: 0.5px;
	left:-1px;
	width: 57px;
	height:28px;
	border: 1.5px solid #E5E5E5;
	border-top:none;
	background: transparent;
	border-radius: 0px 0px 128px 128px;
}
.dianImg{
	width: 40px;
	height: 40px;
	position: absolute;
	border: 1.5px solid white;
	left: 50%;
	top:50%;
	-webkit-transform: translate(-50%,-50%);
	    -ms-transform: translate(-50%,-50%);
	        transform: translate(-50%,-50%);
	border-radius: 50%;
	color: white;
	background: white;
}
.tab-bar {
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  height: 53px;
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  padding-bottom: env(safe-area-inset-bottom);
  z-index: 1000;
  background-image: -o-repeating-radial-gradient(50% 0px ,
  500px 500px ,
  transparent 0%, transparent 28.8px,white 28.8px, white 200%);
  background-image: repeating-radial-gradient(500px 500px at 50% 0px ,
  transparent 0%, transparent 28.8px,white 28.8px, white 200%);
}
.tab-bar-border {
  background-image: -o-repeating-radial-gradient(50% 0px ,
  500px 500px ,
  transparent 0%, transparent 29px,#E6E6E6 29px, #E6E6E6 200%);
  background-image: repeating-radial-gradient(500px 500px at 50% 0px ,
  transparent 0%, transparent 29px,#E6E6E6 29px, #E6E6E6 200%);
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 1.5px;
}
.tab-bar-item {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
      -ms-flex: 1;
          flex: 1;
  text-align: center;
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
      -ms-flex-pack: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
      -ms-flex-align: center;
          align-items: center;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
      -ms-flex-direction: column;
          flex-direction: column;
}
.tab-bar-item:nth-child(2){
	padding-right: 110rpx;
	/* border: 1px solid black; */
}
.coverImg{
  width: 22px;
  height: 22px;
}
.tab-bar-item view {
  font-size: 10px;
  position: relative;
  bottom: -3px;
}
.theDad{
	position: absolute;
	height: calc(99%);
	width: 100%;
	overflow-y: auto;
}

