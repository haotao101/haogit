
.theDad{
	position: absolute;
	width: 100%;
	height: 100%;
}
.container-fill{
	width: 100%;
	height: 100%;
	overflow: hidden;
}
.scroll-fullpage{
	width: 100%;
	height: 100%;
	-webkit-transition: all 0.3s;
	-o-transition: all 0.3s;
	transition: all 0.3s;
}
.active{
	width: 100%;
	height: 100%;
}
.section{
	width: 100%;
	height: 100%;
}
.section-maintitle{
	display: block;
	text-align: center;
	font-size: 50rpx;
	color: #fff;
	font-weight: bold;
	letter-spacing: 10rpx;
	padding-top: 140rpx;
}
.section-subtitle{
	display: block;
	text-align: center;
	font-size: 40rpx;
	color: #fff;
	font-weight: bold;
	letter-spacing: 10rpx;
}
.active .section-maintitle,
.active .section-subtitle{
	-webkit-animation: mymove 0.8s;
	        animation: mymove 0.8s;
}
@-webkit-keyframes mymove{
from {
		-webkit-transform: translateY(-400rpx) scale(0.5) rotateY(90deg);
		        transform: translateY(-400rpx) scale(0.5) rotateY(90deg);
}
to {
		-webkit-transform: translateY(0) scale(1) rotateY(0);
		        transform: translateY(0) scale(1) rotateY(0);
}
}
@keyframes mymove{
from {
		-webkit-transform: translateY(-400rpx) scale(0.5) rotateY(90deg);
		        transform: translateY(-400rpx) scale(0.5) rotateY(90deg);
}
to {
		-webkit-transform: translateY(0) scale(1) rotateY(0);
		        transform: translateY(0) scale(1) rotateY(0);
}
}

